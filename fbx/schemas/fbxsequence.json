{
	"$schema": "http://json-schema.org/draft/2019-09/schema#",
	"description": "A representation of the components required to export an animation asset.",
	"type": "object",
	"patternProperties":
	{ 
		"^.*$": 
		{
		  "allOf": [ { "$ref": "#/definitions/FbxSequence" } ]
		}
	},
	"definitions": {
		"FbxSequence": 
		{
			"type": "object",
			"properties": 
			{
				"__name__": { "type": "string" },
				"__module__": { "type": "string" },
				"name": { "type": "string" },
				"directory": { "type": "string" },
				"startFrame": { "type": "number" },
				"endFrame": { "type": "number" },
				"step": { "type": "number" },
				"exportSetId": { "type": "number" },
				"customScripts": 
				{ 
					"type": "array",
					"items": 
					{ 
						"type": "object",
						"$ref": "#/definitions/CustomScript" 
					}
				}
			}
		},
		"CustomScript": 
		{
			"type": "object",
			"properties": 
			{
				"__name__": { "type": "string" },
				"__module__": { "type": "string" },
				"filePath": { "type": "string" },
				"args": { "type": "array" },
				"kwargs": { "type": "object" }
			}
		}
	}
}